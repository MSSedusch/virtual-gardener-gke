{
  "global": {
    "apiserver": {
      "caBundle": "-----BEGIN CERTIFICATE-----\n...\n-----END CERTIFICATE-----\n",
      "enabled": true,
      "etcd": {
        "servers": "https://virtual-garden-etcd:2379",
        "useSidecar": false
      },
      "featureGates": {},
      "groupPriorityMinimum": 10000,
      "image": {
        "pullPolicy": "Always",
        "repository": "eu.gcr.io/gardener-project/gardener/apiserver",
        "tag": "0.16.0"
      },
      "insecureSkipTLSVerify": false,
      "replicaCount": 1,
      "resources": {
        "limits": {
          "cpu": "300m",
          "memory": "256Mi"
        },
        "requests": {
          "cpu": "100m",
          "memory": "100Mi"
        }
      },
      "serviceAccountName": "gardener-apiserver",
